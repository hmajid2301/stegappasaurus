{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "no-var-requires": false,
    "no-console": [
      "error"
    ]
  },
  "rulesDirectory": []
}